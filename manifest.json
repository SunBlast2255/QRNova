{
    "manifest_version": 3,
    "name": "QRNova",
    "description": "Extension for generating QR codes",
    "version": "1.1",
    "action": {
      "default_popup": "popup/index.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "icons": {
      "16": "images/icons/16.png",
      "32": "images/icons/32.png",
      "48": "images/icons/48.png",
      "64": "images/icons/64.png",
      "128": "images/icons/128.png"
    }
  }